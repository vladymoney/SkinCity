generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id          String   @id @default(cuid())
  steam_id    String   @unique
  username    String
  avatar_url  String
  trade_link  String?
  balance     Int      @default(0)
  created_at  DateTime @default(now())
}
